@model ParkManagment.DTOs.Park.ParkDto

<div>
    <dl>
        <dd>
            Id:
        </dd>
        <dt>
            @Model.Id
        </dt>
        <dd>
            Name:
        </dd>
        <dt>
            @Model.Name
        </dt>
    </dl>
    <span>Are you sure you want to delete this Park?</span>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="DeletePark" class="btn btn-danger" />
    </form>
    <a asp-action="Index" class="btn-group ">Go back to list of Parks </a>
</div>
